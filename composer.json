{
    "name": "dmitryd/typo3-sentry",
    "type": "typo3-cms-extension",
    "description": "Sentry monitoring for TYPO3",
    "homepage": "https://github.com/dmitryd/typo3-sentry",
    "version": "1.0.0",
    "time": "2017-07-13",
    "license": "GPL-3.0+",
    "keywords": [
        "TYPO3",
        "CMS",
        "log",
        "typo3",
        "error",
        "monitoring"
    ],
    "replace": {
        "sentry": "self.version",
        "typo3-ter/sentry": "self.version"
    },
    "require": {
        "typo3/cms-core": ">=7.6.0,<8.7.999",
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "DmitryDulepov\\Sentry\\": "Classes"
        }
    },
    "authors": [
        {
            "name": "Dmitry Dulepov",
            "email": "dmitry.dulepov@gmail.com",
            "homepage": "http://www.dmitry-dulepov.com/"
        }
    ]
}
